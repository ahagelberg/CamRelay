# CamRelay Configuration File Example

[server]
rtsp_port=8554
max_clients_per_stream=10
connection_timeout_seconds=30
buffer_size_kb=1024

[logging]
level=INFO
file_path=logs/camrelay.log
max_file_size_mb=100
max_files=5
console_output=true

[camera.1]
id=camera1
name=Camera 1
rtsp_url=rtsp://<ip-address1>:554/h264Preview_01_sub
username=
password=
enabled=true
max_streams=5
retry_interval_seconds=30
connection_timeout_seconds=10

[camera.2]
id=camera2
name=Camera 2
rtsp_url=rtsp://<ip-address2>:554/h264Preview_01_sub
username=
password=
enabled=true
max_streams=5
retry_interval_seconds=30
connection_timeout_seconds=10
